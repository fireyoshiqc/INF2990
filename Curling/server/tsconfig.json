{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceRoot": "app",
      "sourceMap": true,
      "outDir": "out",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noImplicitAny": true,
	    "noImplicitThis": true,
      "suppressImplicitAnyIndexErrors": true
    }
}
